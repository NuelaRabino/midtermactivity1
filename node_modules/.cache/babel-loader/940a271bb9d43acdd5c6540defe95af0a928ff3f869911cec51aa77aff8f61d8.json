{"ast":null,"code":"import { mapGetters, mapMutations, mapActions } from 'vuex';\nexport default {\n  computed: {\n    ...mapGetters(['books', 'cart', 'totalPrice'])\n  },\n  methods: {\n    ...mapMutations(['addToCart', 'removeFromCart']),\n    ...mapActions(['purchaseAll'])\n  }\n};","map":{"version":3,"names":["mapGetters","mapMutations","mapActions","computed","methods"],"sources":["D:\\midtermactivity1\\src\\components\\ShoppingCart.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mx-auto px-4 py-8\">\r\n    <h2 class=\"text-3xl font-bold mb-4\">Bookstore</h2>\r\n    <div class=\"overflow-x-auto\">\r\n      <table class=\"table-auto w-full border-collapse border border-gray-300\">\r\n        <thead>\r\n          <tr class=\"bg-gray-200\">\r\n            <th class=\"px-4 py-2\">Title</th>\r\n            <th class=\"px-4 py-2\">Price</th>\r\n            <th class=\"px-4 py-2\">Actions</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"(book) in books\" :key=\"book.id\" class=\"border-b border-gray-300\">\r\n            <td class=\"px-4 py-2\">{{ book.title }}</td>\r\n            <td class=\"px-4 py-2\">₱{{ book.price }}</td>\r\n            <td class=\"px-4 py-2\">\r\n              <button @click=\"addToCart(book)\" class=\"bg-green-500 hover:bg-green-600 text-white font-bold py-2 px-4 rounded\">Add to Cart</button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n    <h2 class=\"text-3xl font-bold my-8\">Shopping Cart</h2>\r\n    <div class=\"overflow-x-auto\">\r\n      <table class=\"table-auto w-full border-collapse border border-gray-300\">\r\n        <thead>\r\n          <tr class=\"bg-gray-200\">\r\n            <th class=\"px-4 py-2\">Book Name</th>\r\n            <th class=\"px-4 py-2\">Quantity</th>\r\n            <th class=\"px-4 py-2\">Price</th>\r\n            <th class=\"px-4 py-2\">Total</th>\r\n            <th class=\"px-4 py-2\"></th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"(item, index) in cart\" :key=\"index\" class=\"border-b border-gray-300\">\r\n            <td class=\"px-4 py-2\">{{ item.title }}</td>\r\n            <td class=\"px-4 py-2\">{{ item.quantity }}</td>\r\n            <td class=\"px-4 py-2\">₱{{ item.price }}</td>\r\n            <td class=\"px-4 py-2\">₱{{ item.price * item.quantity }}</td>\r\n            <td class=\"px-4 py-2\">\r\n              <button @click=\"removeFromCart(index)\" class=\"bg-red-500 hover:bg-red-600 text-white font-bold py-2 px-4 rounded\">Remove</button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n    <p class=\"text-xl font-bold mt-4\">Total: ₱{{ totalPrice }}</p>\r\n    <div class=\"mt-4\">\r\n      <button @click=\"purchaseAll\" class=\"bg-blue-500 hover:bg-blue-600 text-white font-bold py-2 px-4 rounded-md\">Purchase All</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters, mapMutations, mapActions } from 'vuex';\r\n\r\nexport default {\r\n  computed: {\r\n    ...mapGetters(['books', 'cart', 'totalPrice'])\r\n  },\r\n  methods: {\r\n    ...mapMutations(['addToCart', 'removeFromCart']),\r\n    ...mapActions(['purchaseAll'])\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAwDA,SAASA,UAAU,EAAEC,YAAY,EAAEC,UAAS,QAAS,MAAM;AAE3D,eAAe;EACbC,QAAQ,EAAE;IACR,GAAGH,UAAU,CAAC,CAAC,OAAO,EAAE,MAAM,EAAE,YAAY,CAAC;EAC/C,CAAC;EACDI,OAAO,EAAE;IACP,GAAGH,YAAY,CAAC,CAAC,WAAW,EAAE,gBAAgB,CAAC,CAAC;IAChD,GAAGC,UAAU,CAAC,CAAC,aAAa,CAAC;EAC/B;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}