{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapGetters, mapActions } from 'vuex';\nexport default {\n  computed: {\n    ...mapGetters(['userAccounts', 'user']),\n    isAdmin() {\n      return this.user && this.user.name === 'Administrator';\n    },\n    users() {\n      return this.userAccounts.filter(user => user.name !== 'Administrator');\n    }\n  },\n  methods: {\n    ...mapActions(['changeUserStatus']),\n    changeStatus(user, newStatus) {\n      user.status = newStatus;\n      this.changeUserStatus({\n        userId: user.id,\n        newStatus\n      });\n    },\n    logoutUser() {\n      // Add logic to log out the user\n      // For example, clearing local storage, etc.\n      console.log('Logging out user');\n      // Redirect to login page\n      this.$router.push('/login');\n    },\n    logout() {\n      this.$store.commit('updateAuthenticationStatus', false);\n      this.$store.commit('setUser', null);\n      this.$router.push('/login');\n    }\n  }\n};","map":{"version":3,"names":["mapGetters","mapActions","computed","isAdmin","user","name","users","userAccounts","filter","methods","changeStatus","newStatus","status","changeUserStatus","userId","id","logoutUser","console","log","$router","push","logout","$store","commit"],"sources":["C:\\Users\\Nuela\\midtermact1\\src\\components\\AdminPanel.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <h2>User Accounts</h2>\r\n      <button v-if=\"isAdmin\" @click=\"logout\">Log Out</button>\r\n      <button v-else @click=\"logoutUser\">Log Out</button>\r\n      <table>\r\n        <thead>\r\n          <tr>\r\n            <th>Name</th>\r\n            <th>Status</th>\r\n            <th>Actions</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"user in users\" :key=\"user.id\">\r\n            <td>{{ user.name }}</td>\r\n            <td :class=\"{ 'verified': user.status === 'verified', 'for-verification': user.status === 'for_verification' }\">{{ user.status }}</td>\r\n            <td>\r\n              <button v-if=\"user.status !== 'verified'\" @click=\"changeStatus(user, 'verified')\">Verify</button>\r\n              <button v-if=\"user.status !== 'for_verification'\" @click=\"changeStatus(user, 'for_verification')\">Unverify</button>\r\n              <button @click=\"logoutUser\">Log Out</button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import { mapGetters, mapActions } from 'vuex';\r\n  \r\n  export default {\r\n    computed: {\r\n      ...mapGetters(['userAccounts', 'user']),\r\n      isAdmin() {\r\n        return this.user && this.user.name === 'Administrator';\r\n      },\r\n      users() {\r\n        return this.userAccounts.filter(user => user.name !== 'Administrator');\r\n      }\r\n    },\r\n    methods: {\r\n      ...mapActions(['changeUserStatus']),\r\n      changeStatus(user, newStatus) {\r\n        user.status = newStatus;\r\n        this.changeUserStatus({ userId: user.id, newStatus });\r\n      },\r\n      logoutUser() {\r\n        // Add logic to log out the user\r\n        // For example, clearing local storage, etc.\r\n        console.log('Logging out user');\r\n        // Redirect to login page\r\n        this.$router.push('/login');\r\n      },\r\n      logout() {\r\n        this.$store.commit('updateAuthenticationStatus', false);\r\n        this.$store.commit('setUser', null);\r\n        this.$router.push('/login');\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n  .verified {\r\n    color: green;\r\n  }\r\n  \r\n  .for-verification {\r\n    color: orange;\r\n  }\r\n  </style>\r\n  "],"mappings":";AA6BE,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAE7C,eAAe;EACbC,QAAQ,EAAE;IACR,GAAGF,UAAU,CAAC,CAAC,cAAc,EAAE,MAAM,CAAC,CAAC;IACvCG,OAAOA,CAAA,EAAG;MACR,OAAO,IAAI,CAACC,IAAG,IAAK,IAAI,CAACA,IAAI,CAACC,IAAG,KAAM,eAAe;IACxD,CAAC;IACDC,KAAKA,CAAA,EAAG;MACN,OAAO,IAAI,CAACC,YAAY,CAACC,MAAM,CA