{"ast":null,"code":"import { mapState } from 'vuex';\nimport LoginForm from './components/LoginForm.vue';\nimport BookList from './components/BookList.vue';\nimport SearchBar from './components/SearchBar.vue';\nimport ShoppingCart from './components/ShoppingCart.vue';\nexport default {\n  name: 'App',\n  components: {\n    LoginForm,\n    BookList,\n    SearchBar,\n    ShoppingCart\n  },\n  computed: {\n    ...mapState(['isAuthenticated', 'user'])\n  }\n};","map":{"version":3,"names":["mapState","LoginForm","BookList","SearchBar","ShoppingCart","name","components","computed"],"sources":["C:\\Users\\Nuela\\midtermact1\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <template v-if=\"!isAuthenticated\">\n      <LoginForm />\n    </template>\n    <template v-else>\n      <div>\n        <h2>Welcome, {{ user.name }}</h2>\n        <p>Status: {{ user.status }}</p>\n        <BookList v-if=\"user.status !== 'for_verification'\" />\n        <SearchBar v-if=\"user.status !== 'for_verification'\" />\n        <ShoppingCart v-if=\"user.status === 'verified'\" />\n        <p v-else>Please wait for verification to access the shopping cart.</p>\n      </div>\n    </template>\n  </div>\n</template>\n\n<script>\nimport { mapState } from 'vuex'\nimport LoginForm from './components/LoginForm.vue'\nimport BookList from './components/BookList.vue'\nimport SearchBar from './components/SearchBar.vue'\nimport ShoppingCart from './components/ShoppingCart.vue'\n\nexport default {\n  name: 'App',\n  components: {\n    LoginForm,\n    BookList,\n    SearchBar,\n    ShoppingCart\n  },\n  computed: {\n    ...mapState(['isAuthenticated', 'user'])\n  }\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style>\n"],"mappings":"AAmBA,SAASA,QAAO,QAAS,MAAK;AAC9B,OAAOC,SAAQ,MAAO,4BAA2B;AACjD,OAAOC,QAAO,